{
  "name": "vscode-xiaoshuo",
  "license": "MIT",
  "version": "1.0.23",
  "displayName": "vscode-xiaoshuo",
  "description": "摸鱼才是上班，不摸鱼那是打工。不会摸鱼的员工难拿年终奖",
  "keywords": [
    "纵横",
    "笔趣阁",
    "wulingshan",
    "reader",
    "阅读",
    "小说",
    "摸鱼",
    "起点"
  ],
  "publisher": "wulingshan",
  "icon": "static/images/icon.png",
  "qna": "https://github.com/Luo-yi-le/vscode-xiaoshuo.git/issues",
  "bugs": "https://github.com/Luo-yi-le/vscode-xiaoshuo.git/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Luo-yi-le/vscode-xiaoshuo.git"
  },
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other",
    "Extension Packs"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [

      {
        "command": "wulingshan.command.zhuishushenqi",
        "title": "追书神器",
        "category": "wulingshan"
      },
      {
        "command": "wulingshan.command.biquge",
        "title": "笔趣阁",
        "category": "wulingshan"
      },
      {
        "command": "wulingshan.command.searchOnline",
        "title": "起点",
        "category": "wulingshan"
      },
      {
        "command": "wulingshan.command.fanqie",
        "title": "番茄",
        "category": "wulingshan"
      },
      {
        "command": "wulingshan.command.zongheng",
        "title": "纵横",
        "category": "wulingshan"
      },
      {
        "command": "wulingshan.command.setChapterOrder",
        "title": "设置章节显示顺序",
        "category": "wulingshan"
      },
      {
        "command": "wulingshan.command.collectBook",
        "title": "收藏本书",
        "category": "wulingshan"
      },
      {
        "command": "wulingshan.command.cancelCollect",
        "title": "取消收藏",
        "category": "wulingshan"
      },
      {
        "command": "wulingshan.command.collectList",
        "title": "刷新",
        "category": "wulingshan"
      },
      {
        "command": "wulingshan.command.clearCollect",
        "title": "清空收藏",
        "category": "wulingshan"
      }
    ],
    "viewsContainers": {
      "activitybar": [{
        "id": "treeView",
        "title": "摸鱼小说家",
        "icon": "static/images/icon.svg"
      }]
    },
    "views": {
      "treeView": [{
          "id": "wulingshan-menu",
          "name": "目录"
        },
        {
          "id": "wulingshan-collect",
          "name": "收藏列表"
        }
      ]
    },
    "menus": {
      "view/title": [{
          "command": "wulingshan.command.collectList",
          "when": "view == wulingshan-collect",
          "group": "navigation@1"
        },
        {
          "command": "wulingshan.command.zhuishushenqi",
          "when": "view == wulingshan-menu",
          "group": "navigation@2"
        },
        {
          "command": "wulingshan.command.biquge",
          "when": "view == wulingshan-menu",
          "group": "navigation@3"
        },
        {
          "command": "wulingshan.command.searchOnline",
          "when": "view == wulingshan-menu",
          "group": "navigation@4"
        },
        {
          "command": "wulingshan.command.setChapterOrder",
          "when": "view == wulingshan-menu",
          "group": "custom@5"
        },
        {
          "command": "wulingshan.command.fanqie",
          "when": "view == wulingshan-menu",
          "group": "custom@6"
        },
        {
          "command": "wulingshan.command.zongheng",
          "when": "view == wulingshan-menu",
          "group": "custom@7"
        },
        {
          "command": "wulingshan.command.clearCollect",
          "when": "view == wulingshan-collect",
          "group": "navigation@1"
        }

      ],
      "view/item/context": [{
          "command": "wulingshan.command.collectBook",
          "when": "view == wulingshan-menu && rwulingshan.panel != collect",
          "group": "custom-item@1"
        },
        {
          "command": "wulingshan.command.cancelCollect",
          "when": "view == wulingshan-collect",
          "group": "custom-item@1"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "wulingshan",
      "properties": {
        "wulingshan.onlineSite": {
          "type": "string",
          "default": "笔趣阁, 起点, 追书神器, 番茄, 纵横",
          "enum": [
            "起点",
            "笔趣阁"
          ],
          "description": "在线搜索来源网站"
        },
        "wulingshan.chapterOrder": {
          "type": "string",
          "default": "顺序",
          "enum": [
            "顺序",
            "倒序"
          ],
          "description": "章节显示顺序"
        },
        "wulingshan.readerViewTitle": {
          "type": "string",
          "default": "${name}",
          "markdownDescription": "根据阅读内容控制窗口标题。`${name}`变量可替换为章节标题"
        }
      }
    }
  },
  "activationEvents": [
    "onView:wulingshan-collect",
    "onCommand:wulingshan.command.collectList"
  ],
  "scripts": {
    "build": "vsce package",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/got": "^9.6.9",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.39.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^8.3.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^4.3.5",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "@types/sqlite3": "^3.1.8",
    "cheerio": "^1.0.0-rc.3",
    "epub": "^1.2.0",
    "got": "^9.6.0",
    "hpagent": "^0.1.2",
    "iconv-lite": "^0.5.0",
    "open": "^7.0.0",
    "sqlite3": "^5.0.2",
    "tough-cookie": "^4.0.0",
    "tslib": "^2.3.0"
  }
}